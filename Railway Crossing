#include <Servo.h>
const int led=3;
const int led1=6;
Servo arhat;
Servo pt2;
void setup() {
arhat.attach(5);
pt2.attach(7);
pinMode(led,OUTPUT);
pinMode(led1,OUTPUT);
}
void loop() {
  digitalWrite(led,LOW);
  digitalWrite(led1,HIGH);
  pt2.write(180);
  arhat.write(0);
  delay(1000);
  pt2.write(90);
  arhat.write(90);
  digitalWrite(led,HIGH);
  digitalWrite(led1,LOW);
  delay(1000);
}
